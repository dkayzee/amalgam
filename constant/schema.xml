<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://optimal-systems.org/ns/dmscloud/schema/v4.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://optimal-systems.org/ns/dmscloud/schema/v4.0/ dmsCloud-schema.xsd">
	<propertyStringDefinition>
